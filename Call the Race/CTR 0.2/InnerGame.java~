class InnerGame  {
  public Double[] votedata= new Double[10];
 public Integer[] popdata= new Integer[10];
 public int turn=1;
  public int turn()
  {
    
    
    
   return turn;
  }
  
  public Double[] votedata()
  {
    Double randv;
    int loop=0;
    for(loop=0; loop<4; loop++)
    {
    randv = (Double)(Math.random() * 30)+10;
    votedata[loop]=randv;
    }
    randv = (Double)(Math.random() * 30)+10;
    votedata[4]=randv;
    for(loop=0; loop<4; loop++)
    {
    randv = (Double)(Math.random() * 30)+10;
    votedata[loop+5]=randv;
    }
    return votedata;
    
    
  }
  public Integer[] population()
  {
    int randp;
    int loop=0;
    for(loop=0; loop<4; loop++)
    {
    randp = (int)(Math.random() * 5500) + 5000;
    popdata[loop]=randp;
    }
    randp = (int)(Math.random() * 60000) + 50000;
    popdata[4]=randp;
    for(loop=0; loop<4; loop++)
    {
    randp = (int)(Math.random() * 5500) + 5000;
    popdata[loop+5]=randp; 
    }
    return popdata;
  }
